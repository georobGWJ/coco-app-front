<div class="container">

  <h1 class="app-title">CoCo: Your College Counselor</h1>
  <h3 class="app-statement">Keeping students on track and parents informed.</h3>

  <hr>

  <br>
  <div class="panel panel-default">
    <div class="panel-heading"><strong>Session Info</strong></div>
    <div class="panel-body" style="background-color: #FEFFFF;">
      <ul>
        <li>
          <strong>Session is authenticated?</strong>  {{session.isAuthenticated}}
        </li>

        <li>
          <strong>Session Contents:</strong>
            <p style="word-wrap: break-word;">{{session-object-formatter session.data.authenticated}}</p>
        </li>
      </ul>
    </div>
  </div>
  <!-- {{location-map location="633 Folsom St., San Francisco, CA"}} -->
  {{outlet}}
</div>
